{
  "name": "download-webpack-plugin",
  "version": "1.0.0",
  "description": "A webpack plugin to load resource from internet instead of local file system to ensure the latest is compiled",
  "files": [
    "/dist"
  ],
  "main": "dist",
  "scripts": {
    "lint": "eslint index.js loader.js",
    "unit": "cross-env BABEL_ENV=test nyc --reporter=lcov --reporter=text mocha test/unit",
    "temp": "cross-env BABEL_ENV=debug mocha debug --require babel-register test/unit/loader.test.js",
    "e2e": "mocha --require babel-register test/e2e/specs.test.js",
    "test": "npm run lint &&npm run unit && npm run e2e",
    "prebuild": "npm test",
    "build": "babel src -d dist",
    "prepublishOnly": "npm build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/roneyrao/download-webpack-plugin"
  },
  "bugs": {
    "url": "https://github.com/roneyrao/download-webpack-plugin"
  },
  "homepage": "https://github.com/roneyrao/download-webpack-plugin",
  "keywords": [
    "webpack",
    "plugin",
    "http",
    "https",
    "ftp",
    "download"
  ],
  "author": "Roney",
  "license": "MIT",
  "dependencies": {
    "cacheable-request-adaptable": "^3.0.2",
    "ftp": "^0.3.10",
    "http-cache-semantics": "^3.8.1",
    "keyv": "^3.0.0",
    "keyv-fs": "^1.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-rewire": "^1.1.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "cross-env": "^5.1.3",
    "deepcopy": "^0.6.3",
    "eslint": "^4.18.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0",
    "file-loader": "^1.1.11",
    "flex-mock-server": "^2.2.3",
    "fs-extra": "^5.0.0",
    "ftp-srv": "^2.16.0",
    "mocha": "^5.0.1",
    "nyc": "^11.4.1",
    "pify": "^3.0.0",
    "sinon": "^4.4.2",
    "sinon-chai": "^2.14.0",
    "webpack": "^3.11.0"
  }
}
